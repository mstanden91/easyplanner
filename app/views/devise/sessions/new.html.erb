<div class="paginaRegistro">
  <div class="container">
    <div class="escrito">

     <div class="logo">

     </div>

     <div class="titulo">
      <h2>Inicia sesión </h2>
    </div>

    <div class="row" class="campos">
      <div class="col-md-10">
        <div class="col-md-offset-3">


          <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
x          <div class="field">
            <%= f.label :email %><br />
            <%= f.email_field :email,  class: "form-control",  placeholder: "Ingresa tu correo ",  autofocus: true %>
          </div>

          <div class="field">
            <%= f.label :password %><br />
            <%= f.password_field :password,  class: "form-control",  placeholder: "Ingresa tu contraseña ", autocomplete: "off" %>
          </div>

          <% if devise_mapping.rememberable? -%>
          <div class="field">
            <%= f.check_box :remember_me %>
            <%= f.label :remember_me %>
          </div>
          <% end -%>

          <div class="actions">
          <%= f.submit "Registrarse" %>
          </div>
          <% end %>

          <%= render "devise/shared/links" %>
        </div>
      </div>
    </div>      
  </div>
</div>
</div>
</div>
