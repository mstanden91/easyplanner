     <div class="row">
          <div class="col-md-5">
              <div class="col-md-offset-1">
              <h3> Selecciona una comuna y su precio de despacho</h3>
                   <%= render 'form' %>
              </div>
            </div>
          </div>


    <div class="col-md-6">
      <div class="col-md-offset-3">

        <div class="panel panel-default">
          <!-- Default panel contents -->
          <div class="panel-heading">
            <h1 class="tÃ­tulo"> Comunas que se Abarcan y Precios de Traslado </h1>

          </div>

          <p id="notice"><%= notice %></p>



          <table class="table">
            <thead>
              <tr>
                <th>Lista Asociada</th>
                <th>Comuna</th>
                <th>Precio traslado</th>
                <th colspan="3"></th>
              </tr>
            </thead>

            <tbody>
              <% @transport_rates.each do |transport_rate| %>
              <tr>
                <td><%= transport_rate.list.name %></td>
                <td><%= transport_rate.comunne.name %></td>
                <td><%= transport_rate.price %></td>
                <td><%= link_to 'Show', transport_rate %></td>
                <td><%= link_to 'Edit', edit_transport_rate_path(transport_rate) %></td>
                <td><%= link_to 'Destroy', transport_rate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
              </tr>
              <% end %>
            </tbody>
          </table>

          <br>



        </div>

      </div>


    </div>

  </div>


