<p id="notice"><%= notice %></p>

<h1>Listing Events</h1>

<table>
  <thead>
    <tr>
      <th>Namekid</th>
      <th>Comunne</th>
      <th>Date</th>
      <th>Timefrom</th>
      <th>Timeto</th>
      <th>Guestkid</th>
      <th>Guestadult</th>
      <th>Address</th>
      <th>Phone</th>
      <th>Agekidsfrom</th>
      <th>Agekidsto</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.namekid %></td>
        <td><%= event.comunne %></td>
        <td><%= event.date %></td>
        <td><%= event.timefrom %></td>
        <td><%= event.timeto %></td>
        <td><%= event.guestkid %></td>
        <td><%= event.guestadult %></td>
        <td><%= event.address %></td>
        <td><%= event.phone %></td>
        <td><%= event.agekidsfrom %></td>
        <td><%= event.agekidsTo %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_user_event_path(current_user) %>
